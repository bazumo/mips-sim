{
  "parser": "babel-eslint",
  "rules": {
    "quotes": 0,
    "strict": 0,
    "require-jsdoc": 0,
    "new-cap": 0,
    "arrow-parens": 0,
    "complexity": ["error", 15],
    "no-multiple-empty-lines": 0,
    "spaced-comment": 0,
    "comma-dangle": 0,
    "padded-blocks": 0,
    "one-var": 0,
    "generator-star-spacing": 0,
    "max-len": [2, {
      "ignoreRegExpLiterals": true,
      "ignoreTemplateLiterals": true,
      "ignoreStrings": true,
      "ignoreTrailingComments": true
      }
    ],
    "no-multi-spaces": [2, {
        "ignoreEOLComments": true
      }
    ],
    "object-curly-spacing": 0
  },
  "extends": "google"
}
